{
  "name": "gatsby-prismic-starter",
  "description": "A lean Gatsby Starter which uses the Headless CMS Prismic",
  "license": "MIT",
  "version": "1.0.0",
  "author": "Sam Logan",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "dev": "gatsby develop -o",
    "dev:host": "gatsby develop --host=0.0.0.0",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "lint:ci": "yarn lint --format junit -o results/eslint/result.xml",
    "format": "prettier \"**/*.md \" --write"
  },
  "dependencies": {
    "gatsby": "^3.6.2",
    "gatsby-plugin-brotli": "^2.0.0",
    "gatsby-plugin-canonical-urls": "^3.6.0",
    "gatsby-plugin-create-client-paths": "^3.6.0",
    "gatsby-plugin-force-trailing-slashes": "^1.0.5",
    "gatsby-plugin-gatsby-cloud": "^2.7.1",
    "gatsby-plugin-image": "^1.6.0",
    "gatsby-plugin-manifest": "^3.6.0",
    "gatsby-plugin-offline": "^4.6.0",
    "gatsby-plugin-react-helmet": "^4.6.0",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-root-import": "^2.0.6",
    "gatsby-plugin-sass": "^4.6.0",
    "gatsby-plugin-sharp": "^3.6.0",
    "gatsby-plugin-sitemap": "^4.2.0",
    "gatsby-source-filesystem": "^3.6.0",
    "gatsby-source-prismic": "^3.3.4",
    "gatsby-transformer-sharp": "^3.6.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.7.1",
    "react-slick": "^0.28.0",
    "sass": "^1.34.1",
    "typeface-lato": "^1.1.13"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.13.8",
    "babel-eslint": "^10.0.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1"
  },
  "keywords": [
    "gatsby",
    "starter",
    "prismic",
    "minimal",
    "gatsby-starter"
  ]
}
